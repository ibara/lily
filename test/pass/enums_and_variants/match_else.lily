enum RGB {
    Red,
    Green,
    Blue
}

var red_count = 0
var else_count = 0
var colors = [Red, Blue, Green]

for i in 0...colors.size()-1: {
    match colors[i]: {
        case Red: red_count += 1
        else: else_count += 1
    }
}

if red_count != 1 || else_count != 2:
    stderr.print("Failed.\n")
